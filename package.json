{
  "name": "edam-repo",
  "version": "0.0.0",
  "description": "The origin of programing: setup scaffold",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dev": "lerna run --parallel dev",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish"
  },
  "keywords": [
    "imcuttle",
    "edam"
  ],
  "lint-staged": {
    "packages/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "author": "imcuttle",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.4.7",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.0",
    "eslint-plugin-typescript": "^0.10.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lerna": "^2.7.1",
    "lerna-changelog": "^0.7.0",
    "lint-staged": "^7.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests?__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/edam.git"
  },
  "bugs": {
    "url": "https://github.com/imcuttle/edam/issues"
  },
  "homepage": "https://github.com/imcuttle/edam#readme"
}
