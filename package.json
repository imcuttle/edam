{
  "name": "edam-repo",
  "version": "0.3.0",
  "description": "The origin of programing: setup scaffold",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "npm run dist && jest",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dist": "lerna run --parallel dist",
    "dev": "lerna run --parallel dev",
    "bootstrap": "lerna bootstrap",
    "prerelease": "npm test",
    "release": "lerna publish --conventional-commits -m 'chore(release): publish'",
    "site": "picidae start -c _site/picidae.config.js",
    "site:build": "picidae build -f --source-map -c _site/picidae.config.js"
  },
  "keywords": [
    "imcuttle",
    "edam"
  ],
  "lint-staged": {
    "packages/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "author": "imcuttle",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^23.3.7",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.4.7",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "conventional-changelog-cli": "^1.3.21",
    "eslint": "^4.19.0",
    "eslint-plugin-typescript": "^0.10.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lerna": "^2.7.1",
    "lint-staged": "^7.0.0",
    "picidae": "^2.2.10",
    "picidae-theme-haier": "0.0.8",
    "picidae-transformer-file-syntax": "^1.0.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).{t,j}s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "tildify": "^1.2.0"
  },
  "repository": "imcuttle/edam",
  "bugs": {
    "url": "https://github.com/imcuttle/edam/issues"
  },
  "homepage": "https://github.com/imcuttle/edam#readme"
}
